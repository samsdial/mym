    main.full.pd_site.panel
        .container.company
            .row.mg_bottom--x50
                .column.column-middle.txt_right.mg_top--x30
                    figure.company
                        img(src="images/company/quality.jpg")
                .column.column-middle
                    .title.cl--cyan.txt_size--em4.mg_bottom--x10
                        h2 Política de calidad
                    .paragraf.cl--darkblue.txt_size--em2
