            .title.cl--white.txt_upper.txt_size--em4
                h2 Contacto
    main.full.bgc--aqua.pd_site
        .container
            .row
                .column.column-middle.bgc--white
                    .contact_form.pd_box--x40
                        .subtitle.subtitle_into--center.slate.cl--darkblue.txt_size--em3.txt_center.txt_upper.mg_bottom--x20
                            h2 Ingrese su solicitud y en pocos momentos le responderemos
                        form#contact
                            .data.select.error
                                select#eco.contac_input
                                    option(value="0") Tipo
                                    option(value="1") Mi mama
                                    option(value="2") Mi papa
                                    option(value="3") Otro
                            .data.fullName.error
                                input.contac_input(type="text", placeholder="Nombre completo")
                            .data.company.error
                                input.contact_input(type="text", placeholder="Empresa")
                            .data.city.error
                                input.contact_input(type="text", placeholder="Ciudad")
                            .data.phone.error
                                input.contact_input(type="number", placeholder="Teléfono")
                            .data.email.error
                                input.contact_input(type="email", placeholder="Correo Electronico")
                            .data.message.erro
                                textarea.contact_input(placeholder="Mensaje:")
