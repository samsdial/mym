    main.full.bgc--slate.pd_site.panel
        .container.company
            .row.mg_bottom--x50
                .column.column-middle.txt_right
                    .title.cl--cyan.txt_size--em4.mg_bottom--x10.txt_capital
                        h2 Estados Financieros
                    .paragraf.cl--darkblue.txt_size--em2
